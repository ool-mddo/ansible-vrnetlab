routing-options {
    rib inet6.0 {
        static {
            route ::/0 discard;
        }
    }
    generate {
        route 0.0.0.0/0 discard;
    }
    router-id {{ item.RouterID }};
    autonomous-system {{ item.AS }};
}
protocols {
    bgp { 
        group EXT { 
            type external;
            export EXPORTOSPF;
{% for bgppeer in item | json_query('BGP[?bgp_group == `EXT`]') %}
            neighbor {{ bgppeer.peer_address }} {
                peer-as {{ bgppeer.peer_as }};
            }
{% endfor %}
        }
        group INT {
            type internal;
{% for bgppeer in item | json_query('BGP[?bgp_group == `INT`]') %}
            local-address {{ bgppeer.local_address }};
            export EXPORTOSPF;
            neighbor {{ bgppeer.peer_address }} {
                peer-as {{ bgppeer.peer_as }};
            }
{% endfor %}
        }
        group INT6 {
            type internal;
{% for bgppeer in item | json_query('BGP[?bgp_group == `INT6`]') %}
            local-address {{ bgppeer.local_address }};
            export EXPORTOSPF;
            neighbor {{ bgppeer.peer_address }} {
                peer-as {{ bgppeer.peer_as }};
            }
{% endfor %}
        }
    }
    ospf {
        export ospf-default;
        area {{ item.OSPFv2.area }} {
{% for ospf_if in item.OSPFv2.interfaces %}
            interface {{ ospf_if }};
{% endfor %}
        }
    }
    ospf3 {
        export ospf3-default;
        area {{ item.OSPFv3.area }} {
{% for ospf_if in item.OSPFv3.interfaces %}
            interface {{ ospf_if }};
{% endfor %}
        }
    }
    lldp {
        interface all;
    }
}
policy-options {
    policy-statement EXPORTOSPF {
        term 1 {
            from protocol [ ospf direct ospf3 ];
            then accept;
        }
    }
    policy-statement ospf-default {
        from {
            route-filter 0.0.0.0/0 exact;
        }
        then accept;
    }
    policy-statement ospf3-default {
        from {
            route-filter ::/0 exact;
        }
        then accept;
    }
}

