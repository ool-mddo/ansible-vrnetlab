                address {{ item | json_query('NetworkInterface[?name==`ge-0/0/0`].ipv6') | first }};
            }
        }
    }
    ge-0/0/1 {
        description {{ item | json_query('NetworkInterface[?name==`ge-0/0/1`].description') | first }};
        unit 0 {
            family inet {
                address {{ item | json_query('NetworkInterface[?name==`ge-0/0/1`].ipv4') | first }};
            }
            family inet6 {
                address {{ item | json_query('NetworkInterface[?name==`ge-0/0/1`].ipv6') | first }};
            }
        }
    }
{% if item.NetworkInterface[2].name == "ge-0/0/2" %}
