    fxp0 {
        unit 0 {
            family inet {
                address 10.0.0.15/24;
            }
        }
    }
    lo0 {
        unit 0 {
            family inet {
                address {{ item | json_query('NetworkInterface[?name==`lo0`].ipv4') | first }};
            }
